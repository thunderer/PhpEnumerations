{
    "name": "thunderer/php-enum",
    "description": "PHP enumeration libraries comparison.",
    "type": "project",
    "license": "MIT",
    "authors": [
        { "name": "Tomasz Kowalczyk", "email": "tomasz@kowalczyk.cc" }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "symfony/console": "^4.4",
        "webmozart/assert": "^1.10",
        "guzzlehttp/guzzle": "^6.5",
        "ocramius/package-versions": "^1.11",
        "twig/twig": "^2.14",

        "thunderer/platenum": "^0.4",
        "bensampo/laravel-enum": "^6",
        "commerceguys/enum": "^1",
        "dasprid/enum": "^1",
        "elao/enum": "^2",
        "eloquent/enumeration": "^6",
        "greg0ire/enum": "^4",
        "konekt/enum": "^4",
        "marc-mabe/php-enum": "^4",
        "myclabs/php-enum": "^1",
        "spatie/enum": "^3",

        "xi/collections": "^0.3",
        "garoevans/php-enum": "^1.2",
        "brysem/phpenums": "^1.0",
        "gerritdrost/phenum": "^1.0",
        "vijinho/enums": "^1.0",
        "vcn/enum": "^1.4",
        "esky/enum": "^1.1",
        "dkd/enumeration": "^0.1",
        "crussell52/php-enum": "^0.1",
        "pulyaevskiy/enum": "^1.2",
        "phpextra/enum": "^1.0",
        "olegstyle/enum": "^1.0",
        "mle86/enum": "^0.8",
        "youngsource/typed-enum": "^1.3",
        "mesavolt/named-enum": "^0.6",
        "wirus15/enum": "^0.5",
        "richard-ejem/enum": "^1.0",
        "php-extended/php-enumerable": "^1.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Thunder\\PhpEnumerations\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "ocramius/package-versions": true
        }
    }
}
